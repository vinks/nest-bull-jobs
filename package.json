{
  "name": "nest-bull-jobs",
  "version": "0.1.4",
  "description": "Bull tasks wrapper for NestJS framework",
  "author": "Konstantin Vinogradov",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "start": "cross-env APP_PORT=3010 ts-node examples/main.ts",
    "start:dev": "nodemon --exec cross-env APP_PORT=3010 ts-node -- examples/main.ts",
    "build": "tsc -p tsconfig.json",
    "lint": "tslint -c tslint.json lib/**/*.ts",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vinks/nest-bull-jobs.git"
  },
  "bugs": {
    "url": "https://github.com/vinks/nest-bull-jobs/issues"
  },
  "homepage": "https://github.com/vinks/nest-bull-jobs",
  "dependencies": {
    "@nestjs/common": "^5.7.4",
    "@nestjs/core": "^5.7.4",
    "bluebird": "^3.5.3",
    "bull": "^3.7.0"
  },
  "devDependencies": {
    "@types/bull": "^3.5.8",
    "@types/node": "^7.0.41",
    "cross-env": "^5.2.0",
    "nodemon": "^1.18.10",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.3",
    "ts-loader": "^5.3.3",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.7.0",
    "tslint": "^5.12.1",
    "typescript": "^2.7.2",
    "uuid": "^3.3.2"
  }
}
